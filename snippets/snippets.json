	"FormWrapped": {
		"prefix": "antform-wrapped",
		"body": [
			"const {getFieldDecorator,getFieldsError, getFieldError, isFieldTouched,validateFields}=this.props.form;\r",
			"const hasFieldError=(fieldname)=>isFieldTouched(fieldname) && getFieldError(fieldname);\r",
			"const hasErrors=(fieldsError)=>Object.keys(fieldsError).some(field => fieldsError[field]);\r",
			"\r",
			"<Form onSubmit={e=>{\r",
			"    e.preventDefault();\r",
			"    validateFields((err, values) => {\r",
			"        if (!err) {\r",
			"            console.log(values);\r",
			"        }\r",
			"    });\r",
		    "}}>\r",
			"    <FormItem label='Field A' validateStatus={hasFieldError('fieldA')} help={hasFieldError('fieldA')||''} >\r",
			"    {\r",
			"        getFieldDecorator('fieldA',{\r",
			"            rules:[{required:true,message:'fieldA required'}],\r",
			"        })(\r",
			"            <Input placeholder='error' />\r",
			"        )\r",
			"    }\r",
			"    </FormItem>\r",
			"\r",
			"    <FormItem label='Field B' validateStatus={hasFieldError('fieldB')} help={hasFieldError('fieldB')||''} >\r",
			"    {\r",
			"        getFieldDecorator('fieldB',{\r",
			"            rules:[{required:true,message:'fieldB required'}],\r",
			"        })(\r",
			"            <Input placeholder='error' />\r",
			"        )\r",
			"    }\r",
			"    </FormItem>\r",
			"\r",
			"    <FormItem>\r",
			"        <Button htmlType='submit' type=\"primary\" size=\"large\" disabled={hasErrors(getFieldsError())}>Submit</Button>\r",
			"    </FormItem>\r",
			"</Form>\r",
			""
		],
		"description": "Create a Form to be wrapped by Form.create()"
	},
